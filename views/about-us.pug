    .layout-container
      // Start Page Banner Area
      .page-banner-area
        .container-fluid
          .row.justify-content-center.align-items-center.g-4(data-cues='slideInUp' data-duration='1000')
            .col-xl-8.col-md-12
              .page-banner-content
                .content.section-title-animation.animation-style
                  h2.title-animation About Us
                  ul.list
                    li
                      a(href='/') Home
                    li About Us
                .image
