              style.
                *{
                margin:0;
                padding:0;
                box-sizing:border-box;
                font-family:sans-serif;
                }
                /* OUTER WRAPPER */
                .kt-touch-wrapper{
                max-width:1300px;
                margin:50px auto;
                padding:10px;
                }
                /* MAIN CARD */
                .kt-touch-card{
                background:#fff;
                border-radius:14px;
                box-shadow:0 8px 30px rgba(0,0,0,0.08);
                padding:30px;
                }
                /* GRID */
                .kt-touch-container{
                display:grid;
                grid-template-columns:1fr 1fr;
                gap:40px;
                align-items:center;
                }
                /* FORM */
                .kt-touch-form-title{
                color:#2CD73A;
                font-size:18px;
                font-weight:600;
                text-align:center;
                margin-bottom:20px;
                }
                .kt-touch-form-group{
                margin-bottom:12px;
                }
                .kt-touch-form-input,
                .kt-touch-form-textarea{
                width:100%;
                padding:10px 14px;
                border:1px solid #d1d9e6;
                border-radius:6px;
                background:#f8fafc;
                font-size:14px;
                outline:none;
                }
                .kt-touch-form-input:focus,
                .kt-touch-form-textarea:focus{
                border-color:#1e4d8c;
                box-shadow:0 0 0 3px rgba(30,77,140,.1);
                }
                .kt-touch-form-textarea{
                min-height:80px;
                resize:none;
                }
                /* CHECKBOX */
                .kt-touch-checkbox-group{
                display:flex;
                gap:10px;
                margin:16px 0;
                }
                .kt-touch-checkbox-input{
                width:18px;
                height:18px;
                accent-color:#1e4d8c;
                }
                .kt-touch-checkbox-label{
                font-size:14px;
                color:#6b7280;
                }
                /* BUTTON */
                .kt-touch-submit-btn{
                width:100%;
                padding:12px;
                background:linear-gradient(90deg, #35F729, #78E588, #2CD73A, #03C0C7);
                color:#fff;
                border:none;
                border-radius:6px;
                font-size:15px;
                font-weight:600;
                cursor:pointer;
                }
                .kt-touch-submit-btn:hover{
                background:#2CD73A;
                }
                /* ILLUSTRATION */
                .kt-touch-illustration{
                text-align:center;
                }
                .kt-touch-section-title{
                color:#2CD73A;
                font-size:34px;
                font-weight:700;
                letter-spacing:2px;
                margin-bottom:8px;
                }
                .kt-touch-title-decoration{
                width:180px;
                height:2px;
                background:linear-gradient(90deg, #35F729, #78E588, #2CD73A, #03C0C7);
                margin:0 auto 25px;
                position:relative;
                }
                .kt-touch-title-decoration:before{
                content:"";
                position:absolute;
                left:50%;
                transform:translateX(-50%);
                top:-6px;
                width:14px;
                height:14px;
                background:#f59e0b;
                border-radius:50%;
                }
                .kt-touch-illustration-img{
                max-width:90%;
                }
                /* TOAST */
                .kt-touch-toast{
                position:fixed;
                top:20px;
                right:20px;
                padding:16px 24px;
                border-radius:8px;
                color:#fff;
                opacity:0;
                transform:translateX(100%);
                transition:.3s;
                z-index:1000;
                }
                .kt-touch-toast.show{
                opacity:1;
                transform:translateX(0);
                }
                .kt-touch-toast.success{background:#10b981;}
                .kt-touch-toast.error{background:#ef4444;}
                /* RESPONSIVE */
                @media(max-width:900px){
                .kt-touch-container{
                grid-template-columns:1fr;
                }
                }
              .kt-touch-wrapper
                .kt-touch-card
                  .kt-touch-container
                    // IMAGE
                    .kt-touch-illustration
                      h1.kt-touch-section-title GET IN TOUCH
                      .kt-touch-title-decoration
                      img.kt-touch-illustration-img(src='images/formimg.svg')
                    // FORM
                    div
                      h2.kt-touch-form-title Share Your Query And Contact
                      form#ktTouchForm
                        .kt-touch-form-group
                          input#ktName.kt-touch-form-input(type='text' placeholder='Name')
                        .kt-touch-form-group
                          input#ktEmail.kt-touch-form-input(type='email' placeholder='Email')
                        .kt-touch-form-group
                          input#ktPhone.kt-touch-form-input(type='tel' placeholder='Phone No')
                        .kt-touch-form-group
                          textarea#ktMessage.kt-touch-form-textarea(placeholder='Message')
                        .kt-touch-checkbox-group
                          input#ktRobot.kt-touch-checkbox-input(type='checkbox')
                          label.kt-touch-checkbox-label(for='ktRobot') I&apos;m not a robot
                        button#ktSubmitBtn.kt-touch-submit-btn SUBMIT
              #ktToast.kt-touch-toast
              script.
                const form=document.getElementById("ktTouchForm");
                const btn=document.getElementById("ktSubmitBtn");
                const toast=document.getElementById("ktToast");
                function showToast(m,t){
                toast.textContent=m;
                toast.className=`kt-touch-toast ${t} show`;
                setTimeout(()=>toast.classList.remove("show"),3000);
                }
                form.addEventListener("submit",async e=>{
                e.preventDefault();
                if(!ktName.value.trim()) return showToast("Enter name","error");
                if(!ktEmail.value.trim()) return showToast("Enter email","error");
                if(!ktMessage.value.trim()) return showToast("Enter message","error");
                if(!ktRobot.checked) return showToast("Confirm you're not a robot","error");
                btn.disabled=true;
                btn.textContent="Sending...";
                await new Promise(r=>setTimeout(r,1000));
                showToast("Message sent successfully","success");
                form.reset();
                btn.disabled=false;
                btn.textContent="SUBMIT";
                });
