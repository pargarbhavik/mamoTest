script.
  document.addEventListener("DOMContentLoaded", function() {
    // Typing animation for "Unlimited"
    const typedTextUnlimited = document.getElementById("typed-text-unlimited");
    const textToType = "Unlimited";
    let charIndex = 0;
    
    function typeText() {
      if (charIndex < textToType.length) {
        typedTextUnlimited.textContent += textToType.charAt(charIndex);
        charIndex++;
        setTimeout(typeText, 150);
      }
    }
    
    if (typedTextUnlimited) {
      setTimeout(typeText, 500);
    }
    
    // Bottom line rotating texts
    const heroRotateText = document.getElementById("hero-rotate-text");
    
    if (!heroRotateText) return;
    
    const texts = [
      "Custom Softwares",
      "Intelligent Software",
      "Innovative Solutions"
    ];
    let index = 0;
    heroRotateText.style.transition = "opacity 1s ease, transform 0.5s ease";
    
    function changeText() {
      heroRotateText.style.opacity = "0";
      heroRotateText.style.transform = "translateY(-20px)";
      
      setTimeout(() => {
        index = (index + 1) % texts.length;
        heroRotateText.textContent = texts[index];
        heroRotateText.style.transform = "translateY(20px)";
        requestAnimationFrame(() => {
          heroRotateText.style.opacity = "1";
          heroRotateText.style.transform = "translateY(0)";
        });
      }, 500);
    }
    
    // Change text every 3 seconds
    setInterval(changeText, 3000);
  });
