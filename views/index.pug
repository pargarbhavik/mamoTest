              #ktToast.kt-touch-toast
              script.
                const form=document.getElementById("ktTouchForm");
                const btn=document.getElementById("ktSubmitBtn");
                const toast=document.getElementById("ktToast");
                const ktName=document.getElementById("ktName");
                const ktEmail=document.getElementById("ktEmail");
                const ktMessage=document.getElementById("ktMessage");
                const ktRobot=document.getElementById("ktRobot");
                function showToast(m,t){
                  toast.textContent=m;
                  toast.className=`kt-touch-toast ${t} show`;
                  setTimeout(()=>toast.classList.remove("show"),3000);
                }
                form.addEventListener("submit",async e=>{
                  e.preventDefault();
                  if(!ktName.value.trim()) return showToast("Enter name","error");
                  if(!ktEmail.value.trim()) return showToast("Enter email","error");
                  if(!ktMessage.value.trim()) return showToast("Enter message","error");
                  if(!ktRobot.checked) return showToast("Confirm you're not a robot","error");
                  btn.disabled=true;
                  btn.textContent="Sending...";
                  await new Promise(r=>setTimeout(r,1000));
                  showToast("Message sent successfully","success");
                  form.reset();
                  btn.disabled=false;
                  btn.textContent="SUBMIT";
                });
              // end the get in touch form
