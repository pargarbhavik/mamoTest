                      form#ktTouchForm
                        .kt-touch-form-group
                          input#ktName.kt-touch-form-input(type='text' placeholder='Name')
                        .kt-touch-form-group
                          input#ktEmail.kt-touch-form-input(type='email' placeholder='Email')
                        .kt-touch-form-group
                          input#ktPhone.kt-touch-form-input(type='tel' placeholder='Phone No')
                        .kt-touch-form-group
                          textarea#ktMessage.kt-touch-form-textarea(placeholder='Message')
                        .kt-touch-checkbox-group
                          input#ktRobot.kt-touch-checkbox-input(type='checkbox')
