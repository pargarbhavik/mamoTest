                    script.
                      (function() {
                        const section = document.getElementById('common-home-sect2-wrap');
                        const carousel = document.getElementById('home-innovate-caro');
                        const slides = carousel.querySelectorAll('.carousel-item');
                        const scrollOverlay = document.querySelector('.common-home-sect2 .scroll-img');
                        let currentSlide = 0;
                        
                        function updateSlide(progress) {
                          const slideIndex = Math.min(Math.floor(progress * slides.length), slides.length - 1);
                          if (slideIndex !== currentSlide) {
                            slides[currentSlide].classList.remove('active');
                            slides[slideIndex].classList.add('active');
                            currentSlide = slideIndex;
                            
                            section.className = section.className.replace(/slide-\d-bg/g, '');
                            section.classList.add('slide-' + (slideIndex + 1) + '-bg');
                          }
                        }
                        
                        function handleScroll() {
                          if (!section) return;
                          const rect = section.getBoundingClientRect();
                          const sectionHeight = section.offsetHeight;
                          const windowHeight = window.innerHeight;
                          
                          if (rect.top <= 0 && rect.bottom >= windowHeight) {
                            const scrollProgress = Math.abs(rect.top) / (sectionHeight - windowHeight);
                            const clampedProgress = Math.max(0, Math.min(1, scrollProgress));
                            
                            updateSlide(clampedProgress);
                          }
                        }
                        
                        let ticking = false;
                        window.addEventListener('scroll', function() {
                          if (!ticking) {
                            window.requestAnimationFrame(function() {
                              handleScroll();
                              ticking = false;
                            });
                            ticking = true;
                          }
                        });
                        
                        handleScroll();
                      })();
