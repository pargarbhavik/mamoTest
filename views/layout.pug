script.
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', 'G-1BESF224BJ');
body(class=bodyClass || 'path-frontpage page-node-type-static-pages')
