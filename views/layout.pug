doctype html
html(lang="en" dir="ltr")
head
meta(charset="utf-8")
meta(name="description" content=metaDescription || "MaMo TechnoLabs is a leading software, web and mobile app development company in Vadodara delivering custom IT solutions to grow your business.")
meta(name="abstract" content="Mamo Technologies empowers businesses through technology, with custom software development, AI/ML, IoT, and blockchain solutions.")
if focusKeywords && focusKeywords.length > 0
  meta(name="keywords" content=focusKeywords.join(', '))
else
  meta(name="keywords" content="Mamo Technologies")
meta(name="viewport" content="width=device-width, initial-scale=1.0")
if canonicalUrl
  link(rel="canonical" href=canonicalUrl)
link(rel="icon" href="/images/favicon.ico" type="image/vnd.microsoft.icon")
title= metaTitle ? metaTitle : (title ? `${title} | MaMo TechnoLabs` : 'MaMo TechnoLabs | Software & Web Development Vadodara')
link(rel="stylesheet" media="all" href="/css/style.css")
link(rel="stylesheet" media="all" href="/css/style.css")

link(rel="stylesheet" href="/css/mobile-menu-fix.css")
