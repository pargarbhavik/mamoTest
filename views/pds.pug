                  script.
                    /* TOGGLE CHAT */
                    const chatToggle = document.getElementById("chatToggle");
                    const chatBox = document.getElementById("chatBox");
                    const chatBody = document.getElementById("chatBody");
                    chatToggle.onclick = () => {
                    chatBox.style.display =
                    chatBox.style.display === "flex" ? "none" : "flex";
                    };
                    /* FIXED COMPANY QUESTIONS */
                    const answers = {
                    "services": "We provide software development, UI/UX design, and digital transformation services.",
                    "contact": "You can contact us through the Contact Us page on our website.",
                    "location": "We are based in Vadodara, India.",
                    "careers": "Yes, we are hiring. Please visit our Careers page.",
                    "internship": "We offer internships for students and freshers.",
                    "about": "We are Mamo Technologies, delivering software excellence.",
                    "support": "Yes, we provide long-term support and maintenance."
                    };
                    function sendMessage(){
                    const input = document.getElementById("userInput");
                    const msg = input.value.trim();
                    if(!msg) return;
                    addMessage(msg, "user");
                    input.value = "";
                    setTimeout(() => {
                    const reply = getReply(msg);
                    addMessage(reply, "bot");
                    }, 700);
                    }
                    function getReply(msg){
                    msg = msg.toLowerCase();
                    for(let key in answers){
                    if(msg.includes(key)) return answers[key];
                    }
                    return "Please ask about our services, careers, contact details, or company info.";
                    }
                    function addMessage(text, type){
                    const div = document.createElement("div");
                    div.className = type === "user" ? "user-msg" : "bot-msg";
                    div.innerHTML = `
                    ${type === "bot" ? `<img src="https://cdn-icons-png.flaticon.com/512/4712/4712027.png" class="avatar">` : ""}
                    <div class="msg-bubble">
                    ${text}
                    <span class="time">${new Date().getHours()}:${String(new Date().getMinutes()).padStart(2,'0')}</span>
                    </div>
                    `;
                    chatBody.appendChild(div);
                    chatBody.scrollTop = chatBody.scrollHeight;
                    }
